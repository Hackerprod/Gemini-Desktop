<Window x:Class="GeminiDesktopClient.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:GeminiDesktopClient"
        xmlns:wv2="clr-namespace:Microsoft.Web.WebView2.Wpf;assembly=Microsoft.Web.WebView2.Wpf"
        mc:Ignorable="d"
        Title="Gemini" Height="720" Width="1280" Icon="gemini_icon.ico"
        WindowStartupLocation="CenterScreen">
    <Grid Background="#202124">
        <wv2:WebView2 Name="webView" Source="https://gemini.google.com" CoreWebView2InitializationCompleted="WebView_CoreWebView2InitializationCompleted" Visibility="Collapsed"/>
    </Grid>
</Window>